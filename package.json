{
  "name": "mbx_spa",
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass"
  ],
  "version": "2.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config bundle/index.js --progress",
    "clean": "rimraf ./dist",
    "clean:dll": "rimraf ./dll",
    "build:dll": "rimraf ./dll && webpack --config bundle/webpack.dll.config.js",
    "build:webpack": "rimraf dist/* && cross-env NODE_ENV=prod webpack --config build/index.js",
    "build": "npm run clean && npm run build:dll && npm run build:webpack",
    "lint:staged": "lint-staged",
    "lint:style": "stylelint 'src/**/*.scss' --syntax scss",
    "lint:eslint": "eslint 'src/**/*.js'",
    "lintFix:style": "stylelint --fix 'src/**/*.scss' --syntax scss",
    "lintFix:eslint": "eslint 'src/**/*.js' --fix"
  },
  "lint-staged": {
    "*.js": [
      "npm run lintFix:eslint",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ],
    "*.scss": [
      "npm run lintFix:style",
      "git add"
    ]
  },
  "pre-commit": "lint:staged",
  "author": "jasonT",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "antd": "^3.5.1",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "echarts": "^4.2.0-rc.2",
    "lodash": "^4.17.11",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "mockjs": "^1.0.1-beta3",
    "moment": "^2.23.0",
    "object-assign": "^4.1.1",
    "prop-types": "^15.6.2",
    "qiniu-js": "^2.5.3",
    "qs": "^6.6.0",
    "react": "^16.8.6",
    "react-content-loader": "^4.0.1",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-markdown": "^1.0.0-rc.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "lint-staged": "^8.1.4",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "1.16.4",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-rational-order": "0.0.4",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-declaration-block-no-ignored-properties": "^1.1.0",
    "stylelint-order": "^2.0.0",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-api-mocker": "^1.5.9",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.3.0",
